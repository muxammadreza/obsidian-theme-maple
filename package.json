{
  "name": "maple",
  "version": "0.8.35-beta.7",
  "description": "elegant theme for Obsidian",
  "scripts": {
    "dev": "node scripts/dev.mjs src/dev.scss",
    "dev:test": "node scripts/dev.mjs src/snippets/test.scss",
    "build": "sass src/index.scss theme.css --no-source-map && node scripts/move.mjs",
    "replace-version": "node scripts/version.mjs && pnpm run build",
    "release": "bumpp -x \"pnpm run replace-version\" --all"
  },
  "keywords": [
    "Obsidian"
  ],
  "author": "subframe7536",
  "license": "MIT",
  "dependencies": {
    "sass": "1.69.7",
    "which": "^4.0.0"
  },
  "type": "module",
  "devDependencies": {
    "bumpp": "^9.5.2"
  }
}
